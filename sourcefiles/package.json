{
  "name": "openwebif-assets",
  "version": "0.0.1",
  "description": "Static assets for OpenWebif interface",
  "main": "index.js",
  "dependencies": {
    "@babel/runtime": "^7.14.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "babel-preset-minify": "^0.5.1",
    "clean-css": "^5.1.2",
    "clean-css-cli": "^5.3.0",
    "commander": "^7.2.0",
    "glob-uglifyjs": "^2.0.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.3.1",
    "sass": "^1.35.1"
  },
  "resolutions": {
    "node-forge": "~0.10.0",
    "yargs-parser": "^20.2.7",
    "glob-parent": "^5.1.2",
    "css-what": "^5.0.1",
    "normalize-url": "^4.5.1",
    "postcss": "^8.2.10"
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "build-classic-js": "npx glob-uglifyjs -c ./classic-glob-uglifyjs.config",
    "uglify-modern": "npx glob-uglifyjs -c ./modern-glob-uglifyjs.config",
    "build-js": "npm run build-prod --prefix modern; npm run-script uglify-modern;",
    "build-css": "node ./_utils/mincss.js -o '../plugin/public/' './modern/css/**/*.css'",
    "test": "echo 'Error: no test specified!' && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/E2OpenPlugins/e2openplugin-OpenWebif.git"
